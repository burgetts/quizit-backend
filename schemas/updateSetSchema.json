{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "description",
        "sideOneName",
        "sideTwoName"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "MCAT amino acids"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "Name and structure of all 20 amino acids"
            ]
        },
        "sideOneName": {
            "type": "string",
            "default": "",
            "title": "The sideOneName Schema",
            "examples": [
                "Term"
            ]
        },
        "sideTwoName": {
            "type": "string",
            "default": "",
            "title": "The sideTwoName Schema",
            "examples": [
                "Definition"
            ]
        }
    },
    "examples": [{
        "name": "MCAT amino acids",
        "description": "Name and structure of all 20 amino acids",
        "sideOneName": "Term",
        "sideTwoName": "Definition"
    }]
}